<!-- src/main/resources/templates/rains.mustache -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Rains</title>
</head>
<body>
<h1>Rain Data</h1>
<table border="1">
    <thead>
    <tr>
        <th>Date</th>
        <th>Amount</th>
    </tr>
    </thead>
    <tbody>
    {{#rains}}
        <tr>
            <td>{{date}}</td>
            <td>{{amount}}</td>
        </tr>
    {{/rains}}
    </tbody>
</table>
</body>
</html>
